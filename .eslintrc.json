{
  "extends": [
    "eslint:recommended"
  ],
  "env": {
    "commonjs": true
  },
  "parserOptions": {
    "ecmaVersion": 6
  },
  "rules": {
    "space-before-function-paren": ["error", "never"],
    "no-multi-spaces": "error",
    "space-before-blocks": "error",
    "indent": ["error", 2]
  }
}
